{
  "name": "ebates-chat-kit",
  "version": "1.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "concurrently \"yarn lint:watch\" \"yarn test:watch\" \"yarn watch\"",
    "lint": "eslint src/**/*.js",
    "lint:watch": "esw src/**/*.js -w",
